{
  "name": "offloc-server",
  "version": "0.0.0",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && nodemon server.js",
    "build": "npm run css-build",
    "css-build": "./bin/build-css",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "mocha",
    "wait-for-deploy": "node ./bin/wait-for-deploy",
    "record-build-info": "node ./bin/record-build-info"
  },
  "engines": {
    "node": "^14.16.1",
    "npm": "^6.14.12"
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci",
      "migrations",
      "node_modules",
      "build-info.json",
      "test"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "applicationinsights": "^1.0.2",
    "azure-arm-storage": "^4.0.0",
    "azure-keyvault": "^3.0.4-preview",
    "azure-storage": "^2.8.2",
    "basic-auth": "^2.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "bunyan-middleware": "^0.8.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.1",
    "ejs": "~2.6.1",
    "express": "~4.16.3",
    "express-request-id": "^1.4.0",
    "generate-password": "^1.4.0",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.5.0",
    "govuk_template_jinja": "^0.24.0",
    "helmet": "^4.5.0",
    "hsts": "^2.1.0",
    "http-errors": "~1.6.3",
    "mkdirp": "^0.5.1",
    "morgan": "~1.9.0",
    "ms-rest-azure": "^2.5.5",
    "nocache": "^2.1.0",
    "node-sass-middleware": "0.11.0",
    "poll-endpoint-for": "^0.0.4",
    "promise-timeout": "^1.3.0",
    "sass-middleware": "^0.0.3"
  },
  "devDependencies": {
    "adm-zip": "^0.4.11",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "mocha": "^7.2.0",
    "mocha-junit-reporter": "^1.17.0",
    "nodemon": "^1.17.4",
    "sinon": "^5.0.7",
    "supertest": "^3.1.0"
  }
}
