{
  "name": "offloc-server",
  "version": "0.0.0",
  "scripts": {
    "start": "node server.js",
    "start:dev": "npm run build && nodemon server.js",
    "build": "npm run css-build",
    "css-build": "./bin/build-css",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "NODE_ENV=test mocha test/*.spec.js test/**/*.spec.js",
    "wait-for-deploy": "node ./bin/wait-for-deploy",
    "record-build-info": "node ./bin/record-build-info"
  },
  "engines": {
    "node": ">= 8.11.1",
    "npm": ">=5.6.0"
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "applicationinsights": "^1.0.2",
    "azure": "^2.2.1-preview",
    "azure-keyvault": "^3.0.4-preview",
    "azure-storage": "^2.8.2",
    "basic-auth": "^2.0.0",
    "bcrypt": "^2.0.1",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "bunyan-middleware": "^0.8.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dotenv": "^5.0.1",
    "ejs": "~2.5.8",
    "express": "~4.16.3",
    "express-request-id": "^1.4.0",
    "govuk-elements-sass": "^3.1.2",
    "govuk_frontend_toolkit": "^7.4.2",
    "govuk_template_jinja": "^0.23.1",
    "helmet": "^3.12.0",
    "hsts": "^2.1.0",
    "http-errors": "~1.6.3",
    "mkdirp": "^0.5.1",
    "morgan": "~1.9.0",
    "ms-rest-azure": "^2.5.5",
    "node-sass-middleware": "0.11.0",
    "passport": "^0.4.0",
    "poll-endpoint-for": "^0.0.4",
    "sass-middleware": "^0.0.3"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "mocha": "^5.1.0",
    "mocha-junit-reporter": "^1.17.0",
    "nodemon": "^1.17.3",
    "sinon": "^4.5.0",
    "supertest": "^3.0.0"
  }
}
